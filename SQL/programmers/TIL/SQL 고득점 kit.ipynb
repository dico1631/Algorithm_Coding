   "source": [
    "# 1. 상위 n개 추출\n",
    "- mySQL\n",
    "```\n",
    "SELECT A\n",
    "FROM B\n",
    "LIMIT n\n",
    "```\n",
    "- Oracle\n",
    "```\n",
    "SELECT A\n",
    "FROM B\n",
    "WHERE ROWNUM <= n\n",
    "```\n",
    "- SQL Server\n",
    "```\n",
    "SELECT TOP n A\n",
    "FROM B\n",
    "```"
   ]
  }
